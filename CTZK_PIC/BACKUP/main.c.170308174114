#pragma interrupt_handler E_comp:24
void E_comp(void)  //模拟比较器中断,总线电压被拉低到28.5V后，立即停电机
{   
  if(PWM1_PIN==1)//收放电机欠压堵转
   {
 	 Motor1Status.s.UnderVoltage=1;
   }
  if(PWM2_PIN==1)//调节电机欠压堵转
   { 
 	Motor2Status.s.UnderVoltage=1;
   } 
   MOTORSTOP  //关电机 
}

